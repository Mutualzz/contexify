{
    "author": "Fadi Khadra <fdkhadra@gmail.com> (https://github.com/fkhadra)",
    "dependencies": {
        "@mutualzz/ui-core": "workspace:*",
        "@mutualzz/ui-web": "workspace:*"
    },
    "description": "Add contextmenu to your react component with ease",
    "devDependencies": {
        "bunchee": "^6.6.2",
        "eslint": "^9.39.1",
        "@mutualzz/eslint-config": "workspace:*",
        "prettier-plugin-organize-imports": "^4.3.0"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**",
        "dist"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "name": "@mutualzz/contexify",
    "peerDependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@types/react": "^17.0.0 || ^18.0.0 || ^19.0.0",
        "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Mutualzz/contexify.git"
    },
    "scripts": {
        "build": "pnpm typecheck && bunchee",
        "dev": "bunchee --watch",
        "prepublishOnly": "pnpm build",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "typecheck": "tsc --noEmit"
    },
    "sideEffects": false,
    "types": "./dist/index.d.ts",
    "version": "6.0.0"
}
